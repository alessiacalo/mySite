<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Navigation Bar Dialog"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <tabs jcr:primaryType="nt:unstructured"
                  sling:resourceType="granite/ui/components/coral/foundation/tabs"
                  maximized="{Boolean}false">
                <items jcr:primaryType="nt:unstructured">
                    <webSite
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Web Site"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns jcr:primaryType="nt:unstructured"
                                     sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                     margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <branding
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Home"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <siteName
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Site Name"
                                                    name="./siteName"/>
                                            <siteText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Site Text"
                                                    name="./siteText"/>
                                            <siteLink
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Site Link"
                                                    nodeTypes="dam:Asset, nt:file, nt:folder, cq:Page,  sling:Folder, sling:OrderedFolder"
                                                    name="./siteLink"
                                                    required="{Boolean}false"
                                                    rootPath="/content/dam"/>
                                        </items>
                                    </branding>
                                    <button
                                            jcr:primaryType="nt:unstructured"
                                            jcr:title="Button"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                        <items jcr:primaryType="nt:unstructured">
                                            <buttonText
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldLabel="Button Text"
                                                    name="./buttonText"/>
                                            <buttonLink
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Button Link"
                                                    nodeTypes="dam:Asset, nt:file, nt:folder, cq:Page,  sling:Folder, sling:OrderedFolder"
                                                    name="./buttonLink"
                                                    required="{Boolean}false"
                                                    rootPath="/content/dam"/>
                                        </items>
                                    </button>
                                    <clickHere
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                            text="Click here for register "
                                            value="true"
                                            name="./clickHere"/>
                                </items>
                            </columns>
                        </items>
                    </webSite>
                    <shopOnline
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Shop Online"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns jcr:primaryType="nt:unstructured"
                                     sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                     margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <shopMultifield
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldDescription="Click + to add a new page"
                                                    fieldLabel="Multifield Shop"
                                                    name="./shopMultifield">
                                                <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./items1">
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <shopText
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                fieldLabel="Shop Text"
                                                                name="./shopText"/>
                                                        <linkShop
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                fieldLabel="Link Shop"
                                                                nodeTypes="dam:Asset, nt:file, nt:folder, cq:Page,  sling:Folder, sling:OrderedFolder"
                                                                name="./linkShop"
                                                                required="{Boolean}true"
                                                                rootPath="/content/dam"/>
                                                    </items>
                                                </field>
                                            </shopMultifield>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </shopOnline>
                    <details
                            jcr:primaryType="nt:unstructured"
                            jcr:title="Details"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns jcr:primaryType="nt:unstructured"
                                     sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                     margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <linkAbout
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="About"
                                                    nodeTypes="dam:Asset, nt:file, nt:folder, cq:Page,  sling:Folder, sling:OrderedFolder"
                                                    name="./linkAbout"
                                                    required="{Boolean}false"
                                                    rootPath="/content/dam"/>
                                            <linkBlog
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Blog"
                                                    nodeTypes="dam:Asset, nt:file, nt:folder, cq:Page,  sling:Folder, sling:OrderedFolder"
                                                    name="./linkBlog"
                                                    required="{Boolean}false"
                                                    rootPath="/content/dam"/>
                                            <linkContact
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Contact"
                                                    nodeTypes="dam:Asset, nt:file, nt:folder, cq:Page,  sling:Folder, sling:OrderedFolder"
                                                    name="./linkContact"
                                                    required="{Boolean}false"
                                                    rootPath="/content/dam"/>
                                            <linkCart
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Shopping Cart"
                                                    nodeTypes="dam:Asset, nt:file, nt:folder, cq:Page,  sling:Folder, sling:OrderedFolder"
                                                    name="./linkCart"
                                                    required="{Boolean}false"
                                                    rootPath="/content/dam"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </details>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>


